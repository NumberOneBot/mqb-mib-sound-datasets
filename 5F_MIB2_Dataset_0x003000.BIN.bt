    printEQFilter(Profile_2[i].Filter5, 2);
    printEQFilter(Profile_1[i].Filter6, 1);
    printEQFilter(Profile_2[i].Filter6, 2);
    printEQFilter(Profile_1[i].Filter7, 1);
    printEQFilter(Profile_2[i].Filter7, 2);
    printDoubleLines();
    Printf("\n");
}

